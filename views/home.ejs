      </select>

      </div>
    </div>
    <div class="form-group">        
      <div class="col-sm-offset-2 col-sm-10">
        <center><button type="submit" class="btn btn-default">Submit</button></center>
      </div>
    </div>
  </form>
</div>
<% } else{ %>
  <center><h1 style="margin-top: 15%;">WELCOME TO GENERO 2K19</h1></center>
  <% } %>
</body>
<script type="text/javascript">
  $('select').selectpicker();
</script>
<script>
  var fullUrl = window.location.href;   
  //alert(fullUrl) ;
  if((fullUrl.split("?")[1]).split("=")[1]=="success"){
      alert("REGISTERED");
  }
  if((fullUrl.split("?")[1]).split("=")[1]=="error"){
      alert("ERROR");
  }
  if((fullUrl.split("?")[1]).split("=")[1]=="attended"){
      alert("ATTENDED");
  }
</script>
</html>
