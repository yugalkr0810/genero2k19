            <option value="Debate">Debate</option>
            <option value="Business_Plan">Business Plan</option>
            <option value="ADZAP">ADZAP</option>
            <option value="Business_Conclave_(flagship_event)">Business Conclave (flagship event)</option>
            <option value="Market_Mayhem">Market Mayhem</option>
            <option value="Beg_Borrow_&Selfie">Beg Borrow & Selfie</option>
            <option value="Business_Quiz">Business Quiz</option>
            <option value="Paper/Poster_Presentation">Paper/Poster Presentation</option>
            <option value="Out_of_Waste">Out of Waste</option>
